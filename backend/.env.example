# Copy this file to .env and set the values before starting the backend
# MongoDB connection string (required)
MONGO_URI=mongodb+srv://<user>:<password>@cluster0.mongodb.net/smackdown?retryWrites=true&w=majority

# Optional: frontend origin (set to your deployed frontend URL in production)
# If not set, the backend will allow all origins (development mode)
FRONTEND_URL=http://localhost:5173

# Optional: PORT (defaults to 5000)
# PORT=5000
